        let availableIds = []; // Keep track of available IDs (including deleted ones)
        let highestId = 2; // Start with 2 as we have products 1 and 2 by default

        // Helper function to reset colors and point displays
        function resetComparison() {
            // Clear all highlights
            document.querySelectorAll('.better-value, .tied-value').forEach(cell => {
                cell.classList.remove('better-value', 'tied-value');
            });

            // Reset all point displays
            document.querySelectorAll('[id$="-total-points"]').forEach(cell => {
                cell.textContent = "-";
            });
        }

        // Get the next available product ID
        function getNextAvailableId() {
            if (availableIds.length > 0) {
                return availableIds.shift(); // Take the first available ID
            } else {
                return ++highestId; // No available IDs, create a new one
            }
        }

        // Get all unique property names except "sku"
        function getPropertyNames() {
            const properties = new Set();
            productData.forEach(product => {
                Object.keys(product).forEach(key => {
                    if (key !== "sku") {
                        properties.add(key);
                    }
                });
            });
            // Make sure "Termék neve" is first
            const propArray = Array.from(properties);
            propArray.sort((a, b) => {
                if (a === "Termék neve") return -1;
                if (b === "Termék neve") return 1;
                return a.localeCompare(b);
            });
            return propArray;
        }

        // Initialize the table with all property rows
        function initializeTable() {
            const tableBody = document.querySelector('#comparison-table tbody');
            const propertyNames = getPropertyNames();

            // Skip "Termék neve" as it's already in the dropdown
            for (let i = 0; i < propertyNames.length; i++) {
                const prop = propertyNames[i];
                if (prop === "Termék neve") continue; // Skip adding row for product name

                const row = document.createElement('tr');
                row.innerHTML = `
                        <td class="property-label">${prop}</td>
                        <td id="product1-${prop.replace(/\s+/g, '-')}" data-product-id="1">-</td>
                        <td id="product2-${prop.replace(/\s+/g, '-')}" data-product-id="2">-</td>
                        <td></td>
                    `;
                tableBody.appendChild(row);
            }

            // Add event listener for the add product button
            document.getElementById('add-product-btn').addEventListener('click', addProductColumn);
        }

        // Add a new product column
        function addProductColumn() {
            const productId = getNextAvailableId(); // Get the next ID (reusing deleted ones if available)

            // Add new header cell
            const headerRow = document.getElementById('header-row');
            const lastHeaderCell = headerRow.lastElementChild;
            const newHeaderCell = document.createElement('th');
            newHeaderCell.setAttribute('data-product-id', productId);

            // Use "Termék X" format
            newHeaderCell.textContent = `Termék ${productId} `;

            const removeButton = document.createElement('button');
            removeButton.className = 'remove-column-btn';
            removeButton.innerHTML = '&times;';
            removeButton.title = 'Termék eltávolítása';
            removeButton.addEventListener('click', function (e) {
                e.stopPropagation(); // Prevent event bubbling
                removeProductColumn(productId);
            });

            newHeaderCell.appendChild(removeButton);

            // Insert before the + button cell
            headerRow.insertBefore(newHeaderCell, lastHeaderCell);

            // Add new dropdown
            const dropdownRow = document.getElementById('dropdown-row');
            const dropdownEmptyCell = dropdownRow.lastElementChild;
            const newDropdownCell = document.createElement('td');
            newDropdownCell.setAttribute('data-product-id', productId);
            newDropdownCell.innerHTML = `
                    <select id="product${productId}" onchange="updateComparison(${productId})">
                        <option value="">Válassz terméket...</option>
                    </select>
                `;
            dropdownRow.insertBefore(newDropdownCell, dropdownEmptyCell);

            // Add new points cell
            const pointsRow = document.getElementById('points-row');
            const pointsEmptyCell = pointsRow.lastElementChild;
            const newPointsCell = document.createElement('td');
            newPointsCell.id = `product${productId}-total-points`;
            newPointsCell.setAttribute('data-product-id', productId);
            newPointsCell.textContent = '-';
            pointsRow.insertBefore(newPointsCell, pointsEmptyCell);

            // Add new cell to each property row
            const tableRows = document.querySelectorAll('#comparison-table tbody tr');
            tableRows.forEach(row => {
                // Skip dropdown and points rows as we've already handled them
                if (row.id === 'dropdown-row' || row.id === 'points-row') return;

                const propName = row.querySelector('.property-label').textContent;
                const emptyCell = row.lastElementChild;
                const newCell = document.createElement('td');
                newCell.id = `product${productId}-${propName.replace(/\s+/g, '-')}`;
                newCell.setAttribute('data-product-id', productId);
                newCell.textContent = '-';
                row.insertBefore(newCell, emptyCell);
            });

            // Populate the new dropdown
            populateDropdown(productId);

            // Reset colors and comparisons when adding a new product
            resetComparison();
        }

        // Remove a product column
        function removeProductColumn(productId) {
            // Add the ID back to available IDs for reuse
            availableIds.push(parseInt(productId));
            availableIds.sort((a, b) => a - b); // Keep them sorted for sequential reuse

            // Find and remove all cells with this product ID
            document.querySelectorAll(`[data-product-id="${productId}"]`).forEach(element => {
                if (element && element.parentNode) {
                    element.parentNode.removeChild(element);
                }
            });

            // Reset and recalculate
            resetComparison();
            compareProducts();
        }

        // Populate a dropdown with products
        function populateDropdown(productId) {
            const dropdown = document.getElementById(`product${productId}`);
            if (!dropdown) return;

            // Add products to dropdown
            productData.forEach(product => {
                const option = document.createElement('option');
                option.value = product["Termék neve"];
                option.textContent = product["Termék neve"];
                dropdown.appendChild(option);
            });
        }

        // Initialize the application when the document is loaded
        document.addEventListener('DOMContentLoaded', function () {
            // Initialize the table with all property rows
            initializeTable();

            // Populate initial dropdowns
            populateDropdown(1);
            populateDropdown(2);
        });

        // Get all product IDs currently in the table
        function getAllProductIds() {
            const ids = new Set();
            document.querySelectorAll('[data-product-id]').forEach(element => {
                ids.add(parseInt(element.getAttribute('data-product-id')));
            });
            return Array.from(ids);
        }

        // Compare all products and update table
        function compareProducts() {
            // Reset all highlights first
            resetComparison();

            // Get all selected products
            const selectedProducts = [];
            const productIds = getAllProductIds();

            productIds.forEach(productId => {
                const productElement = document.getElementById(`product${productId}`);
                if (!productElement) return;

                const productName = productElement.value;
                if (productName) {
                    const product = productData.find(p => p["Termék neve"] === productName);
                    if (product) {
                        selectedProducts.push({
                            id: productId,
                            product: product,
                            totalPoints: 0
                        });
                    }
                }
            });

            if (selectedProducts.length < 2) return; // Need at least 2 products to compare

            const propertyNames = getPropertyNames();

            // Calculate total points for each product
            propertyNames.forEach(prop => {
                if (prop === "Termék neve") return;

                // Find max points for this property among selected products and count how many products have it
                let maxPoints = 0;
                let maxPointsCount = 0;
                const pointsMap = new Map(); // To track points of each product

                // First pass: find max points
                selectedProducts.forEach(selected => {
                    if (selected.product[prop]) {
                        let points = 0;
                        if (typeof selected.product[prop] === 'object' && selected.product[prop].Point !== undefined) {
                            points = selected.product[prop].Point;
                            selected.totalPoints += points;
                        }

                        pointsMap.set(selected.id, points);

                        if (points > maxPoints) {
                            maxPoints = points;
                            maxPointsCount = 1;
                        } else if (points === maxPoints) {
                            maxPointsCount++;
                        }
                    }
                });

                // Second pass: highlight based on whether there's a tie or clear winner
                if (maxPoints > 0) {
                    const isTie = maxPointsCount > 1; // If more than one product has the max points

                    selectedProducts.forEach(selected => {
                        const points = pointsMap.get(selected.id) || 0;
                        if (points === maxPoints) {
                            const cell = document.getElementById(`product${selected.id}-${prop.replace(/\s+/g, '-')}`);
                            if (cell) {
                                // Use tied-value class for ties, better-value for clear winners
                                cell.classList.add(isTie ? 'tied-value' : 'better-value');
                            }
                        }
                    });
                }
            });

            // Update total points for each product
            selectedProducts.forEach(selected => {
                const pointsCell = document.getElementById(`product${selected.id}-total-points`);
                if (pointsCell) {
                    pointsCell.textContent = selected.totalPoints.toFixed(1) + ' pont';
                }
            });

            // Find highest total score and count how many products have it
            let maxTotalPoints = 0;
            let maxTotalPointsCount = 0;

            selectedProducts.forEach(selected => {
                if (selected.totalPoints > maxTotalPoints) {
                    maxTotalPoints = selected.totalPoints;
                    maxTotalPointsCount = 1;
                } else if (selected.totalPoints === maxTotalPoints) {
                    maxTotalPointsCount++;
                }
            });

            // Highlight total score - use tied-value for ties
            const isTotalTie = maxTotalPointsCount > 1;
            selectedProducts.forEach(selected => {
                if (selected.totalPoints === maxTotalPoints) {
                    const pointsCell = document.getElementById(`product${selected.id}-total-points`);
                    if (pointsCell) {
                        pointsCell.classList.add(isTotalTie ? 'tied-value' : 'better-value');
                    }
                }
            });
        }

        // Update comparison when a product is selected
        function updateComparison(productId) {
            // Reset all colors first
            resetComparison();

            const selectedProduct = document.getElementById(`product${productId}`).value;
            const propertyNames = getPropertyNames();

            propertyNames.forEach(prop => {
                if (prop === "Termék neve") return; // Skip product name

                const cellId = `product${productId}-${prop.replace(/\s+/g, '-')}`;
                const cell = document.getElementById(cellId);

                if (!cell) return; // Skip if cell doesn't exist

                if (selectedProduct) {
                    const product = productData.find(p => p["Termék neve"] === selectedProduct);

                    if (product && product[prop]) {
                        // Check if property is an object with Value
                        if (typeof product[prop] === 'object' && product[prop].Value !== undefined) {
                            cell.textContent = product[prop].Value;
                        } else {
                            cell.textContent = product[prop];
                        }
                    } else {
                        cell.textContent = "-";
                    }
                } else {
                    cell.textContent = "-";
                    const pointsCell = document.getElementById(`product${productId}-total-points`);
                    if (pointsCell) {
                        pointsCell.textContent = "-";
                    }
                }
            });

            // Update comparisons
            compareProducts();
        }
    </script>
